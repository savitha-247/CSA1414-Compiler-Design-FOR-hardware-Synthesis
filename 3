#include <stdio.h>

int main() {
    char expression[100];

    printf("Enter an expression: ");
    scanf("%s", expression);

    printf("\nOperator Precedence (PEMDAS) means:\n");
    printf("1. Parentheses ( )\n");
    printf("2. Exponentiation ^\n");
    printf("3. Multiplication * and Division /\n");
    printf("4. Addition + and Subtraction -\n");

    printf("\nGiven Expression: %s\n", expression);
    printf("Evaluation will follow the above order of precedence.\n");

    return 0;
}

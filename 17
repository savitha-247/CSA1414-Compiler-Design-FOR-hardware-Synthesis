
/* Filename: url.l */

%{
#include <stdio.h>
%}

%%

"https://"([a-zA-Z0-9._/-])+   { printf("Valid URL: %s\n", yytext); }
"http://"([a-zA-Z0-9._/-])+    { printf("Valid URL: %s\n", yytext); }
.|\n                           { printf("Invalid URL\n"); }

%%

int yywrap() { return 1; }

int main() {
    printf("Enter URL: ");
    yylex();
    return 0;
}

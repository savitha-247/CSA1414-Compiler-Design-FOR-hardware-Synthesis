%{
#include <stdio.h>
%}

%%
if|else|while|int|switch|for|char    { printf("it is a keyword\n"); }
[a-zA-Z_][a-zA-Z0-9_]*               { printf("%s is IDENTIFIER\n", yytext); }
[ \t\n]                              { /* ignore whitespace */ }
.                                    { printf("Unknown character: %s\n", yytext); }
%%

int yywrap() {
    return 0;
}

int main() {
    yylex();
    return 0;
}
